{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PCForecast",
  "type": "object",
  "required": ["id", "clientId", "month", "expectedCalls", "expectedDistributions"],
  "properties": {
    "id": { "type": "string" },
    "clientId": { "type": "string" },
    "fundId": { "type": ["string", "null"] },
    "month": { "type": "string", "format": "date" },
    "expectedCalls": { "type": "number" },
    "expectedDistributions": { "type": "number" },
    "net": { "type": "number" },
    "confidence": { 
      "type": "string",
      "enum": ["low", "medium", "high", "manual"]
    },
    "notes": { "type": ["string", "null"] },
    "createdAt": { "type": "string", "format": "date-time" },
    "updatedAt": { "type": "string", "format": "date-time" }
  }
}
