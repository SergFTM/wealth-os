{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Role",
  "type": "object",
  "properties": {
    "id": { "type": "string" },
    "clientId": { "type": "string" },
    "name": { "type": "string" },
    "builtIn": { "type": "boolean" },
    "description": { "type": "string" },
    "clientSafePolicyJson": { "type": "string" },
    "color": { "type": "string" },
    "priority": { "type": "integer" },
    "createdAt": { "type": "string", "format": "date-time" }
  },
  "required": ["id", "name"]
}
